  "type": "project",
  "require": {
    "php": ">=7.4",
    "mongodb/mongodb": "1.17.0",
    "firebase/php-jwt": "^6.0",
    "vlucas/phpdotenv": "^5.5",
    "cloudinary/cloudinary_php": "^2.0"
  },
  "autoload": {
